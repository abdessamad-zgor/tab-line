{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["builds/**"]
    },
    "type-check": {},
    "publish": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "deploy": {
      "outputs": ["deployments/**"]
    },
    "deploy:prod": {
      "outputs": ["deployments/prod/**"]
    }
  }
}
